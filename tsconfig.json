{
	"compilerOptions": {
		"moduleResolution": "node",
		"module": "ESNext",
		"target": "ESNext",
		"isolatedModules": true,
		"noEmit": true,
		"skipLibCheck": true,

		"allowSyntheticDefaultImports": true,
		"strict": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"strictBindCallApply": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"strictPropertyInitialization": true,
		"types": ["bun-types"]
	}
}
